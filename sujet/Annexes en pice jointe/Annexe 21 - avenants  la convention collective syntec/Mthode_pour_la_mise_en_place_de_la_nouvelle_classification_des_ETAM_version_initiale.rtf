{\rtf1 \ansi {\*\userprops {\propname jforCreatedThisOn}\proptype30 {\staticval Sun Jan 11 14:21:34 CET 2015}{\propname jforVersion}\proptype30 {\staticval jfor V0.7.2rc1 - see http://www.jfor.org}}{\colortbl; \red0\green0\blue0;\red255\green255\blue255;\red255\green0\blue0;\red0\green255\blue0;\red0\green0\blue255;\red0\green255\blue255;\red255\green0\blue255;\red255\green255\blue0;\red0\green0\blue128;\red0\green128\blue128;\red0\green128\blue0;\red128\green0\blue128;\red128\green0\blue0;\red128\green128\blue0;\red128\green128\blue128;\red192\green192\blue192;}{\fonttbl; {\f0 arial}{\f1 symbol}{\f2 times new roman}}{\*\listtable{\list\listtemplateid595297590\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid601395490}}{\*\listoverridetable{\listoverride\listid601395490\listoverridecount0\ls1}}{\*\listtable{\list\listtemplateid2077753174\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid599471745}}{\*\listoverridetable{\listoverride\listid599471745\listoverridecount0\ls2}}{\*\listtable{\list\listtemplateid-1061729546\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid599086996}}{\*\listoverridetable{\listoverride\listid599086996\listoverridecount0\ls3}}{\*\listtable{\list\listtemplateid-1890243114\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid597932749}}{\*\listoverridetable{\listoverride\listid597932749\listoverridecount0\ls4}}{\*\listtable{\list\listtemplateid1601187935\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid682577507}}{\*\listoverridetable{\listoverride\listid682577507\listoverridecount0\ls5}}{\*\listtable{\list\listtemplateid-884352770\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid679114767}}{\*\listoverridetable{\listoverride\listid679114767\listoverridecount0\ls6}}{\*\listtable{\list\listtemplateid620274911\listsimple{\listlevel\levelnfc23\leveljc0\levelfollow0\levelstartat1\levelspace0\levelindent360{\leveltext\'01\'b7 ?;}{\levelnumbers}\f2}{\listname ;}\listid687194494}}{\*\listoverridetable{\listoverride\listid687194494\listoverridecount0\ls7}}\paperw11905 \paperh16837 \margt1133 \margb1133 \margl1133 \margr1133 \sectd {\qr {\fs24 Le 11 janvier 2015}\par }{{ }\par }{{ }\par }{{ }\par }{{ }\par }{{ }\par }{\qc {\fs24 Accord }\par }{\qc {\b \fs24 Accord du 15 décembre 1987 relatif à la méthode pour la mise en place de la nouvelle classification des ETAM.}\par }{{ }\par }{{ }\par }{{ }\par }{{ }\par }{{ }\par }{\ql {}{\b \fs24 1. POURQUOI UNE NOUVELLE CLASSIFICATION ? }{}\par }{{ }\par }\pard \fi-360 \li360 {{\*\pn \pnlvlblt \ilvl0 \ls1 \pnindent360 \pnf1 {\pndec \f2 \pntxtb \'b7 }}\ls1 {\ql {\pntext \f1 \'b7 \tab }{\fs24 Créé par Accord 1987-12-15 étendu par arrêté du 13 avril 1988 JORF 27 avril 1988}\par }}\pard {{ }\par }{\ql {\fs24 Le système de classification actuellement en vigueur a vieilli :}\par }{{ }\par }{\ql {\fs24 certaines fonctions n\rquote étaient pas prises en compte.}\par }{{ }\par }{{ }\par }{\ql {\fs24 Le nouveau système consiste essentiellement en un cadre général et souple permettant de procéder à une répartition des emplois en référence à des critères :}\par }{{ }\par }{{ }\par }{\ql {\fs24 - communs à toutes les filières professionnelles ;}\par }{{ }\par }{{ }\par }{\ql {\fs24 - prédéterminés dans leur formulation, leur nombre et leur gradation.}\par }{{ }\par }{{ }\par }{\ql {\fs24 Il n\rquote y a pas de correspondance systématique générale entre les anciens et les nouveaux coefficients.}\par }{{ }\par }{{ }\par }{\ql {}{\b \fs24 2. PRESENTATION GENERALE }{}\par }{{ }\par }\pard \fi-360 \li360 {{\*\pn \pnlvlblt \ilvl0 \ls2 \pnindent360 \pnf1 {\pndec \f2 \pntxtb \'b7 }}\ls2 {\ql {\pntext \f1 \'b7 \tab }{\fs24 Créé par Accord 1987-12-15 étendu par arrêté du 13 avril 1988 JORF 27 avril 1988}\par }}\pard {{ }\par }{\ql {\fs24 Ce nouveau système organise la hiérarchie des fonctions quel que soit la filière ou le métier considéré : }\par }{\ql {\fs24 - suivant une grille unique pour toutes les fonctions ; }\par }{\ql {\fs24 - en trois types de fonctions : }\par }{\ql {\fs24 1. Fonctions d\rquote exécution ; }\par }{\ql {\fs24 2. Fonctions d\rquote études ou de préparation ; }\par }{\ql {\fs24 3. Fonctions de conception ou de gestion élargie, }\par }{\ql {\fs24 elles-mêmes subdivisées en positions internes (12 positions au total). }\par }{\ql {\fs24 Dans la forme et le contenu du nouveau système, ont été recherchées : }\par }{\ql {\fs24 - l\rquote homogénéité : les types de fonctions et positions sont identifiés et définis à partir d\rquote une méthode unique ; }\par }{\ql {\fs24 - la cohérence : les positions sont définies non seulement en tant que telles, mais également par rapport aux positions immédiatement inférieures et immédiatement supérieures concrétisant ainsi la progression ; }\par }{\ql {\fs24 - l\rquote adaptabilité : les définitions présentent un caractère souple permettant de mieux saisir ou de mieux suivre l\rquote enrichissement des tâches qui est susceptible de découler des modifications de l\rquote exercice des métiers. }\par }{\ql {\fs24 Les définitions des types de fonctions découlent toutes d\rquote une conception unique qui analyse les fonctions au travers de leurs aspects fondamentaux : }\par }{{ }\par }{{ }\par }{\ql {\fs24 Objet du travail ; }\par }{{ }\par }{{ }\par }{\ql {\fs24 Modèles d\rquote actions ; }\par }{{ }\par }{{ }\par }{\ql {\fs24 Démarches intellectuelles. }\par }{\ql {\fs24 Pour en tirer des définitions générales portant sur les : }\par }{{ }\par }{{ }\par }{\ql {\fs24 Contenu ; }\par }{{ }\par }{{ }\par }{\ql {\fs24 Caractéristiques communes (autonomie, responsabilité...) ; }\par }{{ }\par }{{ }\par }{\ql {\fs24 Connaissances requises. }\par }{\ql {\fs24 Les définitions de positions dérivent également d\rquote une conception commune : elles sont établies principalement à partir de la complexité et de la difficulté du travail à accomplir. }\par }{{ }\par }{{ }\par }{\ql {}{\b \fs24 3. APPLICATIONS}{ }\par }{{ }\par }\pard \fi-360 \li360 {{\*\pn \pnlvlblt \ilvl0 \ls3 \pnindent360 \pnf1 {\pndec \f2 \pntxtb \'b7 }}\ls3 {\ql {\pntext \f1 \'b7 \tab }{\fs24 Créé par Accord 1987-12-15 étendu par arrêté du 13 avril 1988 JORF 27 avril 1988}\par }}\pard {\qc {\fs24 3.1. Quelques principes  }\par }{\ql {\fs24 L\rquote application de la nouvelle classification suppose que soient respectés les principes suivants : }\par }{\ql {\fs24 - les salariés sont classés en se fondant sur les caractéristiques et les exigences requises par les fonctions qu\rquote ils occupent :  }\par }{\ql {\fs24 c\rquote est-à-dire que la hiérarchie des salariés découle de la hiérarchie de leurs fonctions et non de la hiérarchie de leurs seules aptitudes ; }\par }{\ql {\fs24 - les fonctions seront affectées d\rquote un nouveau coefficient : on ne saurait perdre de vue que tout nouveau système de classification est novateur et donc doit pouvoir se traduire par une nouvelle hiérarchie des fonctions ; }\par }{\ql {\fs24 - les définitions contenues dans la convention constituent la référence officielle pour classer chaque fonction.  }\par }{\qc {\fs24 3.2. Procédure d\rquote application  }\par }{\ql {\fs24 3.2.1 Schéma de l\rquote approche proposée :  }\par }{\ql {\fs24 1. Recenser les filières existantes ;  }\par }{\ql {\fs24 2. Recenser les fonctions par filières ;  }\par }{\ql {\fs24 3. Décrire les fonctions par rapport aux critères ;  }\par }{\ql {\fs24 4. Classer par rapport aux types de fonctions ;  }\par }{\ql {\fs24 5. Classer par rapport aux positions ;  }\par }{\ql {\fs24 6. Vérifier la cohérence générale du classement ;  }\par }{\ql {\fs24 7. Positionner les salariés ;  }\par }{\ql {\fs24 8. Vérifier la cohérence individuelle ;  }\par }{\ql {\fs24 9. Présenter.  }\par }{\ql {\fs24 Les entreprises pourront trouver avantage à procéder à des séances d\rquote information (pour les points 1 à 6 de la procédure) et à appuyer les analyses individuelles par des procédures de dialogue et de concertation (points 7 et 8).  }\par }{\ql {\fs24 3.2.2 Explication  }\par }{\ql {\fs24 Au niveau de l\rquote entreprise, il y a lieu de :  }\par }{\ql {\fs24 1. Recenser, compte tenu de l\rquote organisation de l\rquote entreprise, les différentes filières existantes.  }\par }{\ql {\fs24 Définition de la filière :  }\par }{\ql {\fs24 La filière comprend l\rquote ensemble des fonctions qui correspondent à des degrés divers de difficulté d\rquote une même spécialité professionnelle et qui, par conséquent, s\rquote inscrivent à divers coefficients de l\rquote échelle hiérarchique de la classification générale.  }\par }{\ql {\fs24 Exemples :  }\par }{\ql {\fs24 Secrétariat, comptabilité, documentation, bureau de dessin, laboratoire, exploitation informatique.  }\par }{\ql {\fs24 2. Recenser dans chaque filière les fonctions caractéristiques telles qu\rquote elles sont effectivement occupées. Par exemple, pour la filière secrétariat, on peut recenser les fonctions suivantes :  }\par }{\ql {\fs24 secrétaire de département, secrétaire d\rquote un groupe d\rquote ingénieurs, secrétaire débutante...  }\par }{\ql {\fs24 On pourra se référer aux définitions générales ou particulières à la filière si elles ont été établies.  }\par }{\ql {\fs24 Cependant, des difficultés pourront être rencontrées pour identifier la filière à laquelle appartient une fonction : sur ce point, il faut que la réponse apportée soit souple et permette facilement, suivant les circonstances, le passage d\rquote une filière à l\rquote autre.  }\par }{\ql {\fs24 En effet, la notion fondamentale est la notion de type de fonction et de position, et celle-ci est fixée en se référant à des critères cohérents entre eux et homogènes quelles que soient les filières.  }\par }{\ql {\fs24 3. Les décrire en faisant ressortir notamment le degré d\rquote autonomie, l\rquote étendue des responsabilités, la difficulté des tâches, les connaissances requises.  }\par }{\ql {\fs24 4. Rechercher le type de fonction dans lequel entre la fonction à analyser. Pour cela, se reporter aux définitions du nouveau système de classification : en effet, il y a un seuil de qualification important entre chacun des 3 types et celui-ci doit être aisément repérable à l\rquote aide des critères suivants : }\par }{\ql {\fs24 - objet du travail ; }\par }{\ql {\fs24 - modèles d\rquote action et démarches intellectuelles ; }\par }{\ql {\fs24 - autonomie : nature des instructions hiérarchiques et contrôle des résultats ; }\par }{\ql {\fs24 - responsabilités ; }\par }{\ql {\fs24 -connaissances requises.  }\par }{\ql {\fs24 5. Préciser ensuite la position à l\rquote intérieur du type de fonction. Cette identification se fait principalement à l\rquote aide des critères suivants de la nouvelle classification : }\par }{\ql {\fs24 - complexité, difficulté du travail à accomplir ; }\par }{\ql {\fs24 - autonomie ; }\par }{\ql {\fs24 - compétences requises, expérience.  }\par }{\ql {\fs24 Pour effectuer ce classement, on pourra se référer : }\par }{\ql {\fs24 - au tableau comparatif des types de fonction (annexe I) ; }\par }{\ql {\fs24 - au tableau des positions (annexe II).  }\par }{\ql {\fs24 6. Vérifier la cohérence générale du classement par fonction et par filière compte tenu du contexte de l\rquote entreprise.  }\par }{\ql {\fs24 7. Placer les salariés dans la hiérarchie des fonctions et positions préalablement établies.  }\par }{\ql {\fs24 Pour qu\rquote un salarié soit situé dans un groupe et une position donnés, il faut qu\rquote il réponde à chacun des critères requis et pas seulement à l\rquote un d\rquote entre eux.  }\par }{\ql {\fs24 Ainsi, un salarié ne peut prétendre être classé dans le type de fonction 3 du seul fait qu\rquote il est titulaire du BTS, encore faut-il qu\rquote il assume intégralement les responsabilités correspondant à ce type de fonction.  }\par }{\ql {\fs24 Dans la lecture des définitions relatives aux types de fonctions II et III, il y a lieu de ne pas se méprendre. Certaines caractéristiques s\rquote appliquent à la lettre aussi bien à des positions de cadres qu\rquote à des positions de coordination de travaux ou de projet relevant traditionnellement des ETAM. En pratique, chaque profession a su fixer une limite entre cadres et non cadres prenant généralement en compte la difficulté des problèmes traités et les connaissances requises, la faculté d\rquote adaptation que donne une certaine culture ou encore l\rquote étendue des responsabilités assumées. Il est nécessaire de bien reconnaître cette limite, variable d\rquote un corps de métier à l\rquote autre et de la respecter au cours du travail de classification.  }\par }{\ql {\fs24 3.2.3 Illustration sur quelques filières  }\par }{\ql {\fs24 En annexe 3 figure un exemple d\rquote application de la nouvelle classification pour quelques fonctions le plus couramment rencontrées dans les entreprises.  }\par }{\ql {\fs24 Cet exemple présente un positionnement moyen des fonctions, qui doit correspondre à la plupart des situations sans être impératif.  }\par }{\ql {\fs24 Les appellations figurant dans cet exemple n\rquote ont aucun caractère normatif, chaque entreprise ayant ses propres appellations.  }\par }{\qc {\fs24 3.3. Conséquences pratiques  }\par }{\ql {\fs24 Rémunération  }\par }{\ql {\fs24 La convention collective prévoit les rémunérations minimales par coefficient.  }\par }{\ql {\fs24 En aucun cas, son application ne pourra être cause de diminution de la rémunération d\rquote un salarié. }\par }{\ql {\fs24 A chaque coefficient peut correspondre une plage de salaires déterminée par la politique de rémunération de l\rquote entreprise. En particulier, rien n\rquote interdit que le salaire maximum pratiqué à un coefficient donné soit supérieur au salaire de début du coefficient supérieur.  }\par }{\ql {\fs24 Conséquence à l\rquote égard des régimes de retraite et de prévoyance  }\par }{\ql {\fs24 Aucune modification individuelle n\rquote interviendra dans l\rquote affiliation aux régimes de retraite et de prévoyance. }\par }{{ }\par }{\ql {}{\b \fs24 4. STRUCTURE DE LA GRILLE DE CLASSIFICATION }{}\par }{{ }\par }\pard \fi-360 \li360 {{\*\pn \pnlvlblt \ilvl0 \ls4 \pnindent360 \pnf1 {\pndec \f2 \pntxtb \'b7 }}\ls4 {\ql {\pntext \f1 \'b7 \tab }{\fs24 Créé par Accord 1987-12-15 étendu par arrêté du 13 avril 1988 JORF 27 avril 1988}\par }}\pard {{\trowd \clvmgf \clmgf \clpadt2 \clpadft3 \clpadb2 \clpadfb3 \clpadl2 \clpadfl3 \clpadr2 \clpadfr3 \clbrdrt\brdrs \clbrdrb\brdrs \clbrdrl\brdrs \clbrdrr\brdrs \brdrw20 \qc \cellx3200 \ql \clvmgf \clmgf \clpadt2 \clpadft3 \clpadb2 \clpadfb3 \clpadl2 \clpadfl3 \clpadr2 \clpadfr3 \clbrdrt\brdrs \clbrdrb\brdrs \clbrdrl\brdrs \clbrdrr\brdrs \brdrw20 \qc \cellx6400 \ql \clvmgf \clmgf \clpadt2 \clpadft3 \clpadb2 \clpadfb3 \clpadl2 \clpadfl3 \clpadr2 \clpadfr3 \clbrdrt\brdrs \clbrdrb\brdrs \clbrdrl\brdrs \clbrdrr\brdrs \brdrw20 \qc \cellx9600 \ql \clbrdrt \clbrdrr \ql \cellx9602 \ql \intbl \qc {{\fs24 FONCTIONS  }}\qc \cell \intbl \qc {{\fs24 POSITIONS  }}\qc \cell \intbl \qc {{\fs24 COEFFICIENTS }}\qc \cell \ql \intbl \cell \row }{\trowd \clvmgf \clmgf \clpadt2 \clpadft3 \clpadb2 \clpadfb3 \clpadl2 \clpadfl3 \clpadr2 \clpadfr3 \clbrdrt\brdrs \clbrdrb\brdrs \clbrdrl\brdrs \clbrdrr\brdrs \brdrw20 \qc \cellx3200 \ql \clvmgf \clmgf \clpadt2 \clpadft3 \clpadb2 \clpadfb3 \clpadl2 \clpadfl3 \clpadr2 \clpadfr3 \clbrdrt\brdrs \clbrdrb\brdrs \clbrdrl\brdrs \clbrdrr\brdrs \brdrw20 \qc \cellx6400 \ql \clvmgf \clmgf \c